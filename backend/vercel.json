{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/static"
      },
      {
        "src": "styles/*",
        "use": "@vercel/static"
      },
      {
        "src": "scripts/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }